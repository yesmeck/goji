Component({
  properties: {
    vdom: {
      type: Object,
      value: {}
    }
  },
  observers: {
    vdom: function () {
      <%# FIXME: have not finished event handler %>
      if (!this.data.vdom) {
        return;
      }
      const props = this.data.vdom.props
      for (let k in props) {
        if (typeof props[k] === 'function') {
          this[k] = props[k]
        }
      }
    },
  },
  options: {
    addGlobalClass: true
  }
})
