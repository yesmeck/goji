<block wx:if="{{vdom.type === '$$REMAX_TYPE_TEXT'}}">
  {{vdom.text}}
</block>

<block wx:elif="{{vdom.type === 'view'}}">
  <view class="{{vdom.props.className}}" bindtap="{{vdom.props.onClick||''}}">
    <fard wx:for="{{vdom.children}}" wx:key="" vdom="{{item}}" />
  </view>
</block>

<block wx:elif="{{vdom.type === 'text'}}">
  <text class="{{vdom.props.className}}" bindtap="{{vdom.props.onClick||''}}">
    <%# <fard wx:for="{{vdom.children}}" wx:key="" vdom="{{item}}" /> %>
    {{vdom.children[0].text}}
  </text>
</block>


<block wx:elif="{{vdom.type === 'button'}}">
  <button class="{{vdom.props.className}}" bindtap="{{vdom.props.onClick||''}}">
    <fard wx:for="{{vdom.children}}" wx:key="" vdom="{{item}}" />
  </button>
</block>

<block wx:elif="{{vdom.type === 'image'}}">
  <image class="{{vdom.props.className}}" src="{{vdom.props.src}}" />
</block>
